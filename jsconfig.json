{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "./*"
      ]
    },
    "moduleResolution": "node",
    "checkJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}
